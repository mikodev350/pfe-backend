{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "conversations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::conversation.conversation",
      "mappedBy": "participants"
    },
    "messages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::message.message",
      "mappedBy": "expediteur"
    },
    "parcours": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::parcour.parcour",
      "mappedBy": "users_permissions_user"
    },
    "modules": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::module.module",
      "mappedBy": "users_permissions_user"
    },
    "lessons": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lesson.lesson",
      "mappedBy": "users_permissions_user"
    },
    "resources": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::resource.resource",
      "mappedBy": "users_permissions_user"
    },
    "phoneNumber": {
      "type": "biginteger"
    },
    "wilaya": {
      "type": "string"
    },
    "dateOfBirth": {
      "type": "date"
    },
    "address": {
      "type": "string"
    },
    "profil": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::profil.profil",
      "mappedBy": "users_permissions_user"
    },
    "educations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::education.education",
      "mappedBy": "users_permissions_user"
    },
    "experiences": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::experience.experience",
      "mappedBy": "users_permissions_user"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "TEACHER",
        "STUDENT"
      ],
      "required": true
    },
    "conversation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::conversation.conversation",
      "mappedBy": "admin"
    },
    "isadminConversation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::conversation.conversation",
      "mappedBy": "admin"
    },
    "devoir": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::devoir.devoir",
      "mappedBy": "teacher"
    },
    "assignations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::assignation.assignation",
      "mappedBy": "etudiant"
    },
    "quizzes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quiz.quiz",
      "mappedBy": "auteur"
    }
  }
}
